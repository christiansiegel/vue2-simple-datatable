{
  "name": "vue2-simple-datatable",
  "version": "0.6.0",
  "description": "Simple, basic, fast DataTable Component for Vue.js 2.0",
  "main": "dist/vue2-simple-datatable.js",
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "build:lib": "cross-env NODE_ENV=production rollup -c",
    "build:examples": "cross-env NODE_ENV=production rollup --config examples/advanced/rollup.config.js",
    "build": "npm run test && npm run build:lib && npm run build:examples",
    "build:commit": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christiansiegel/vue2-simple-datatable.git"
  },
  "bugs": {
    "url": "https://github.com/christiansiegel/vue2-simple-datatable/issues"
  },
  "homepage": "https://github.com/christiansiegel/vue2-simple-datata#readme",
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable"
  ],
  "author": "Christian Siegel",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "cross-env": "^5.0.5",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-preprocessor": "^6.0.1",
    "rollup": "^0.64.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-license": "^0.8.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "v-debounce": "^0.1.2",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  }
}
